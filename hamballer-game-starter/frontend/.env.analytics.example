# Analytics Configuration
# Copy this file to .env.local and fill in your API keys

# General Analytics Settings
VITE_ANALYTICS_ENABLED=true

# Helika Analytics
VITE_HELIKA_ENABLED=true
VITE_HELIKA_API_KEY=your_helika_api_key_here
VITE_HELIKA_GAME_ID=hamballer-testnet
VITE_HELIKA_ENDPOINT=https://api.helika.io/v1

# zkMe Analytics
VITE_ZKME_ENABLED=true
VITE_ZKME_APP_ID=your_zkme_app_id_here
VITE_ZKME_API_KEY=your_zkme_api_key_here
VITE_ZKME_ENDPOINT=https://api.zk.me/v1

# Development Settings
# Set to true to see analytics events in console
VITE_ANALYTICS_DEBUG=true